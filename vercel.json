{
    "devCommand": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "buildCommand": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "installCommand": "npm install --legacy-peer-deps",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://cqi.ustp.edu.ph/dev/Api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}